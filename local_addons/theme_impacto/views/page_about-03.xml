<?xml version="1.0" encoding="utf-8" ?>
<odoo>

<template name="About Us 3" id="about-03">
<t t-call="website.layout">
<div class="page-wrapper oe_structure">
  <section class="inner-intro dark-bg" style="background-image: url('/theme_impacto/static/src/img/bg/bg-01.jpg'); background-size: cover;">
    <div class="container">
      <div class="row intro-title">
        <div class="col-md-12 text-center"><h1 class="title text-white">About Us Style 3</h1></div>
        <div class="col-md-12">
          <div class="row">
            <div class="col-md-12 text-center mt-1">
              <ul class="page-breadcrumb">
                <li><a href="javascript:void(0)">Home</a></li>
                <li><span>About Us Style 3</span> </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <section class="section-md">
    <div class="container">
      <div class="row valign">
        <div class="col-md-6">
          <img class="img-responsive" src="/theme_impacto/static/src/img/about/img-05.jpg" alt="" />
        </div>
        <div class="col-md-5 col-md-offset-1" data-valign-overlay="middle">
          <div class="heading-block">
            <div class="sub-heading">ABOUT US</div>
            <h2 class="title">We create things people love.</h2>
          </div>
          <p class="lead">We make websites are the number one ranked design and marketing team. We build unique and beautiful websites that will make you more successful.</p>
          <div class="row">
            <div class="col-md-6">
              <ul class="list round ">
                <li>Web Design</li>
                <li>UI/UX Design</li>
                <li>Identity Branding</li>
                <li>Photography </li>
              </ul>
            </div>
            <div class="col-md-6">
              <ul class="list round ">
                <li>Web Design</li>
                <li>UI/UX Design</li>
                <li>Identity Branding</li>
                <li>Photography </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <section class="section-md dark-bg pos-r" style="background-image: url('/theme_impacto/static/src/img/about/bg-02.jpg'); background-size: cover;">
    <div class="container">
      <div class="row">
        <div class="col-md-8 col-md-offset-2 text-center">
          <div class="heading-block">
            <div class="sub-heading">We're Impacto Agency</div>
            <h2>We are creative and interactive business agency, we make creative projects</h2>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-md-4">
          <div class="feature icon-bg text-center theme-feature px-2">
            <div class="feature-icon theme-text"><i class="fa fa-paint-brush" aria-hidden="true"></i></div>
            <div class="feature-info">
              <h5>Website Design</h5>
              <p>Web design encompasses many different skills and disciplines in the production of websites.</p>
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="feature icon-bg text-center theme-feature px-2">
            <div class="feature-icon theme-text"><i class="fa fa-user" aria-hidden="true"></i></div>
            <div class="feature-info">
              <h5>Customer Support</h5>
              <p>It is a range of customer services to assist customers in making cost effective and correct use of a product.</p>
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="feature icon-bg text-center theme-feature px-2">
            <div class="feature-icon theme-text"><i class="fa fa-cog" aria-hidden="true"></i></div>
            <div class="feature-info">
              <h5>Development</h5>
              <p>The process of economic and social transformation that is based on complex cultural factors.</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <section class="section-sm">
    <div class="container">
      <div class="row">
        <!-- Align center -->
        <div class="col-md-3">
          <div class="counter text-center">
            <div class="counter-info">
              <span class="timer" data-to="700" data-speed="1000">700</span>
              <label>Hours Of Work</label>
            </div>
          </div>
        </div>
        <div class="col-md-3">
          <div class="counter text-center">
            <div class="counter-info">
              <span class="timer" data-to="250" data-speed="1000">250</span>
              <label>Awords Wininng</label>
            </div>
          </div>
        </div>
        <div class="col-md-3">
          <div class="counter text-center">
            <div class="counter-info">
              <span class="timer" data-to="400" data-speed="1000">400</span>
              <label>Completed Projects</label>
            </div>
          </div>
        </div>
        <div class="col-md-3">
          <div class="counter text-center">
            <div class="counter-info">
              <span class="timer" data-to="355" data-speed="1000">355</span>
              <label>Happy Clients</label>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <hr/>
  <section class="section-md pb-0">
    <div class="container">
      <div class="row ">
        <!-- Accordion Shadow color-->
        <div class="col-md-6">
          <div class="heading-block">
            <div class="sub-heading">WHO WE ARE</div>
            <h2>We make creative projects for awesome clients</h2>
          </div>
          <p class="mb-2">But I must explain to you how all this mistaken idea of denouncing pleasure and praising pain was born.</p>
          <div class="skill">
            <div class="skill-bar">
              <div class="progress-bar" data-percent="96" data-delay="0" data-type="%">
                <div class="progress-title">General Consulting</div>
              </div>
            </div>
            <div class="skill-bar">
              <div class="progress-bar" data-percent="80" data-delay="0" data-type="%">
                <div class="progress-title">Construction Management</div>
              </div>
            </div>
            <div class="skill-bar">
              <div class="progress-bar" data-percent="60" data-delay="0" data-type="%">
                <div class="progress-title">Design &amp; Build</div>
              </div>
            </div>
          </div>
        </div>
        <div class="col-md-6" >
          <img class="img-responsive" src="/theme_impacto/static/src/img/about/img-06.png" alt="" />
        </div>
      </div>
    </div>
  </section>
</div>
</t>
</template>
 <record id="about-03_record" model="website.page">
        <field name="url">/page-about-03</field>
        <field name="website_published">True</field>
        <field name="view_id" ref="about-03"/>
</record>
</odoo>