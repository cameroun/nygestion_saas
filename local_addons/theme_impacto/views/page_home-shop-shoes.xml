<?xml version="1.0" encoding="utf-8" ?>
<odoo>
<template name="Home Shop Shoes" id="home-shop-shoes">
<t t-call="website.layout">
<div class="page-wrapper oe_structure">
  <section class="as-animated-slider">
    <div id="carousel-1" class="carousel slide  banner indicators-right" data-ride="carousel" data-arrow="none">
      <!-- Indicators -->
      <ol class="carousel-indicators">
        <li data-target="#carousel-1" data-slide-to="0" class="active"></li>
        <li data-target="#carousel-1" data-slide-to="1" class=""></li>
      </ol>
      <!-- Wrapper for slides -->
      <div class="carousel-inner" role="listbox">
        <div class="item active">
          <div class="slide-item" style="background-image: url(/theme_impacto/static/src/img/shoes/banner-01.jpg);">
            <div class="slider-text">
              <div class="container">
                <div class="row valign">
                  <div class="col-md-7">
                    <img src="/theme_impacto/static/src/img/shoes/banner-01.png" class="img-responsive" alt="" />
                  </div>
                  <div class="col-md-5" data-valign-overlay="middle">
                    <div class="shop-banner p-0 white-text">
                      <label class="shop-lable lable-light size-md text-uppercase l-space-6">New Collection</label>
                      <h1 class="title-large text-uppercase l-space-10">Summer 2018</h1>
                      <p class="text-light  l-space-1">FREE SHIPPING FOR ORDERS OVERS $199 </p>
                      <a class="link btn-light fill link-md link-light f-weight-600" href="#">Shop Now</a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="item">
          <div class="slide-item" style="background-image: url(/theme_impacto/static/src/img/shoes/banner-02.jpg);">
            <div class="slider-text">
              <div class="container">
                <div class="row valign">
                  <div class="col-md-5" data-valign-overlay="middle">
                    <div class="shop-banner p-0">
                      <label class="shop-lable lable-dark size-md text-uppercase l-space-6">New Collection</label>
                      <h1 class="title-large text-uppercase l-space-10">Summer 2018</h1>
                      <p class="text-dark  l-space-1">FREE SHIPPING FOR ORDERS OVERS $199 </p>
                      <a class="link btn-dark fill link-md link-light f-weight-600" href="#">Shop Now</a>
                    </div>
                  </div>
                  <div class="col-md-7">
                    <img src="/theme_impacto/static/src/img/shoes/banner-02.png" class="img-responsive" alt="" />
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <section class="section-sm info-section">
    <div class="container">
      <div class="row">
        <div class="col-md-3">
          <div class="shop-info">
            <div class="info-icon" style="color: #f5435b;"><i class="fa fa-mobile" aria-hidden="true"></i></div>
            <div class="info-text">
              <h5 class="title">Free Shipping</h5>
              <p>All Order Over 40$</p>
            </div>
          </div>
        </div>
        <div class="col-md-3">
          <div class="shop-info">
            <div class="info-icon" style="color: #eeba30;"><i class="fa fa-envelope-o" aria-hidden="true"></i></div>
            <div class="info-text">
              <h5 class="title">Secure Payments</h5>
              <p>100% Secure Payments</p>
            </div>
          </div>
        </div>
        <div class="col-md-3">
          <div class="shop-info">
            <div class="info-icon" style="color: #e66f47;"><i class="fa fa-clock-o" aria-hidden="true"></i></div>
            <div class="info-text">
              <h5 class="title">Support Customer</h5>
              <p>Support 24 x 7</p>
            </div>
          </div>
        </div>
        <div class="col-md-3">
          <div class="shop-info last">
            <div class="info-icon" style="color: #a5d2e4;"><i class="fa fa-user" aria-hidden="true"></i></div>
            <div class="info-text">
              <h5 class="title">Customer Review</h5>
              <p>Support 24 x 7</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <hr/>
  <section class="section-sm">
    <div class="container">
      <div class="row">
        <div class="col-md-4 text-center">
          <img src="/theme_impacto/static/src/img/shoes/img-01.png" class="img-responsive" alt="" />
          <h5 class="mt-2">Building Brand</h5>
          <p>Lorem ipsum dolor s it amet conse ctetur adipisicing elit sed do eiusmod...</p>
        </div>
        <div class="col-md-4 text-center">
          <img src="/theme_impacto/static/src/img/shoes/img-02.png" class="img-responsive" alt="" />
          <h5 class="mt-2">Building Brand</h5>
          <p>Lorem ipsum dolor s it amet conse ctetur adipisicing elit sed do eiusmod...</p>
        </div>
        <div class="col-md-4 text-center">
          <img src="/theme_impacto/static/src/img/shoes/img-03.png" class="img-responsive" alt="" />
          <h5 class="mt-2">Building Brand</h5>
          <p>Lorem ipsum dolor s it amet conse ctetur adipisicing elit sed do eiusmod...</p>
        </div>
      </div>
    </div>
  </section>
  <section class="section-sm" style="background-image: url('/theme_impacto/static/src/img/shoes/bg-01.jpg'); background-position: center center; background-size: cover;">
    <div class="container">
      <div class="row">
        <div class="col-md-6 text-left">
          <div class="shop-banner">
            <div class="shop-banner-inner align-m">
              <label class="shop-lable l-space-7 size-xs f-weight-600 text-uppercase">E-Commerce Fashion Site</label>
              <h3 class="title-size-md f-weight-700" style="line-height: 1.3em;">Up To 40% Off Final Sale Items</h3>
              <a class="link link-lg f-weight-800" href="#">Shop Now</a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <section class="section-sm">
    <div class="container">
      <div class="row">
        <div class="col-md-4 text-center">
          <div class="shop-banner white-text" style="height: 210px;">
            <div class="shop-banner-image" style="background-image: url(/theme_impacto/static/src/img/shoes/shop-banner-01.jpg); background-position: center center;"></div>
            <div class="shop-banner-inner align-m">
              <label class="l-space-3  f-weight-100">OFF EVERYTHING</label>
              <h3 class="title-size-sm text-light f-weight-700 l-space-3">SAVE 65%</h3>
              <a class="link btn-light fill link-sm link-light f-weight-400" href="#">Shop Now</a>
            </div>
          </div>
        </div>
        <div class="col-md-4 text-center">
          <div class="shop-banner white-text" style="height: 210px;">
            <div class="shop-banner-image" style="background-image: url(/theme_impacto/static/src/img/shoes/shop-banner-02.jpg); background-position: center center;"></div>
            <div class="shop-banner-inner align-m">
              <label class="l-space-3  f-weight-100">OFF EVERYTHING</label>
              <h3 class="title-size-sm text-light f-weight-700 l-space-3">SAVE 65%</h3>
              <a class="link btn-light fill link-sm link-light f-weight-400" href="#">Shop Now</a>
            </div>
          </div>
        </div>
        <div class="col-md-4 text-center">
          <div class="shop-banner white-text" style="height: 210px;">
            <div class="shop-banner-image" style="background-image: url(/theme_impacto/static/src/img/shoes/shop-banner-03.jpg); background-position: center center;"></div>
            <div class="shop-banner-inner align-m">
              <label class="l-space-3  f-weight-100">OFF EVERYTHING</label>
              <h3 class="title-size-sm text-light f-weight-700 l-space-3">SAVE 65%</h3>
              <a class="link btn-light fill link-sm link-light f-weight-400" href="#">Shop Now</a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
<script>
  $(window.document).ready(function(){
    $("body").addClass("home-shop-shoes");
  });
</script>
</div>
</t>
</template>
<record id="home-shop-shoes_record" model="website.page">
        <field name="url">/home-shop-shoes</field>
        <field name="website_published">True</field>
        <field name="view_id" ref="home-shop-shoes"/>
</record>
</odoo>